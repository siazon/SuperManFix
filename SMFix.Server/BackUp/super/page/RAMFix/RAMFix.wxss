/* page/RAMFix.wxss */
@import "/common/weui.wxss";

.phonechs {
  display: inline-block;
  background: white;
  margin: 10rpx 0;
  padding: 10rpx 0;
}

.leftbox {
  float: left;
  width: 50%;
}

.rightbox {
  float: right;
  width: 50%;
}

.rad {
  margin: 20rpx 0 0 30rpx;
}

radr {
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.info {
  background: white;
}
.fixs {
  
  margin-top: 10rpx;
  margin-bottom: 20rpx;
  background: white;
  display: grid;
  grid-template-rows: 50% 50%;
  grid-template-columns: 40% 60%;
  padding: 20rpx 20rpx;
}
.gap{
    margin-top: 10rpx;
}
.cart-footer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  padding: 0 100rpx 0 50rpx;
  box-sizing: border-box;
  background: #353639;
  color: #fff;
}
.cart-toatl-price {
  float: left;
  width: 120rpx;
}
.btnNext {
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  background-size: 100%;
  height: 80rpx;
  line-height: 80rpx;
  width: 280rpx;
  font-size: 40rpx;
  text-align: center;
  text-align-last: center;
  background: #db413f;
  border-radius: 3%;
 
}
.title{
    line-height: 70rpx;
  box-sizing: border-box;
  padding: 0 20rpx 0 20rpx;
}