@import '../common/common.wxss';

page {
  background-color: #f0f0f6;
  height: 100%;
  width: 100%;
}

swiper {
  height: 421.5rpx;
}

swiper-item image {
  width: 100%;
  height: 100%;
}

.select-title, .newest-title {
  width: 100%;
  padding: 30rpx 0;
  height: 40rpx;
  color: #ab956d;
  text-align: center;
}
.btnLab-cell{
padding: 20rpx 0 0 0;
}
.select-top-small {
  width: 375rpx;
  height: 375rpx;
  float: left;
}

.select-bottom, .select-top {
  width: 100%;
  height: 375rpx;
}

.select-top-small image, .select-bottom image, .select-top-small navigator,
.select-bottom navigator {
  display: block;
  width: 100%;
  height: 100%;
}

.newest-box {
  padding: 0 40rpx;
}

.newest-box .newest-list {
  display: inline-block;
  width: 325rpx;
  height: 325rpx;
  margin: 0 20rpx 20rpx 0;
  border-radius: 10px;
  text-align: center;
  background: #f5f6f5;
}

.newest-box .newest-list:nth-child(2n) {
  margin-right: 0;
}

.newest-box .newest-list image {
  width: 175rpx;
  height: 175rpx;
  margin: 20rpx 0 10rpx;
}

.newest-box .newest-list .newest-text {
  font-size: 32rpx;
}

.scoll {
  background: white;
  padding: 5rpx 0 5rpx 20rpx;
}

.ps {
  background: #f5f6f5;
  text-align: center;
  margin: -2rpx 0 0 0;
  padding: 20rpx 0 20rpx 0;
  font-size: 25rpx;
  color: gray;
}

.btn {
  background: #db423f;
  color: #fff;
  margin: 30rpx;
  width: 60%;
}
.btnr {
  background: #2270c9;
  color: #fff;
  margin: 30rpx;
  width: 60%;
}


.btnLab-F {
  padding: 20rpx 44rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.btnLab-Img {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.btnLab-Lab {
  font-size: 25rpx;
  display: flex;
}

.btnLab-wx {
  margin: 50rpx 20rpx 10rpx 40rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 30rpx;
  text-align: center;
}

.swp {
  height: 300rpx;
}

.help {
  display: flex;
  flex-direction: row;
  background: #f5f5f5;
  align-items: center;
  text-align: center;
}

.orders {
  position: relative;
  padding: 30rpx 20rpx;
  font-size: 30rpx;
}

.olist {
  background: white;
  margin: 20rpx 0;
  padding: 20rpx 20rpx 50rpx 20rpx;
}

.name {
  position: absolute;
  left: 30rpx;
  text-align: center;
}

.fault {
  position: absolute;
  left: 100rpx;
  text-align: center;
}

.time {
  position: absolute;
  left: 520rpx;
  text-align: center;
}
